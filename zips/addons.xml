<?xml version="1.0" encoding="UTF-8"?>
<addons>
<addon id="plugin.video.fvideo" version="6.0.4" name="FVideo" provider-name="Choehn">
<requires>
  <import addon="xbmc.python" version="3.0.0"/>
</requires>
<extension point="xbmc.python.pluginsource" library="main.py">
  <provides>video</provides>
</extension>
<extension point="xbmc.addon.metadata">
  <summary lang="en">summary: FVideo</summary>
  <description lang="en_GB">description: FVideo</description>
  <disclaimer lang="en_GB">disclaimer: Free sample videos are provided by FShare</disclaimer>
  <assets>
    <icon>icon.png</icon>
    <fanart>fanart.jpg</fanart>
    <screenshot>resources\screenshot-01.jpg</screenshot>
    <screenshot>resources\screenshot-02.jpg</screenshot>
    <screenshot>resources\screenshot-03.jpg</screenshot>
  </assets>
  <news>news: Updated with latest artwork metadata</news>
</extension>
</addon>

<addon id="repository.choehn" name="Choehn Repo" version="1.0.1" provider-name="Choehn">
    <extension point="xbmc.addon.repository" name="Example Repository">
        <dir>
            <info compressed="false">https://raw.githubusercontent.com/taikhoanonlinevn/repository.choehn/master/zips/addons.xml</info>
            <checksum>https://raw.githubusercontent.com/taikhoanonlinevn/repository.choehn/master/zips/addons.xml.md5</checksum>
            <datadir zip="true">https://raw.githubusercontent.com/taikhoanonlinevn/repository.choehn/master/zips/</datadir>
        </dir>
    </extension>
    <extension point="xbmc.addon.metadata">
        <summary>Repository Choehn</summary>
        <description>Choehn</description>
        <disclaimer></disclaimer>
        <platform>all</platform>
        <assets>
            <icon>icon.png</icon>
            <fanart>fanart.jpg</fanart>
        </assets>
    </extension>
</addon>
</addons>
